package array;

import java.util.ArrayList;

public class Ex04 {
	public static void main(String[] args) {

		Person hong = new Person("홍길동", 25);
		Person park = new Person("박보검", 35);
		Person lee = new Person("이미자", 52);
		
		ArrayList<Person> arr = new ArrayList<Person>();
		
		arr.add(hong);
		arr.add(park);
		arr.add(lee);
		
		
	}
}
