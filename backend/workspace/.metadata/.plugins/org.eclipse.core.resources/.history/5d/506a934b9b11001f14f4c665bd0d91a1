
import java.util.Scanner;

public class tyquiz0514 {

	public static void main(String[] args) {

		// Scanner 객체 생성
		Scanner sc = new Scanner(System.in);

		int row = 0;
		int col = 0;
		int [][] arr;
		
		int minValue = 1;
		int maxValue = 10;
		int userValue = 0;
		
		do {
			System.out.println("행의 크기를 입력하세요. (정수 1 ~ 10 사이의 수) :");
			userValue = sc.nextInt();
			System.out.println("열의 크기를 입력하세요. (정수 1 ~ 10 사이의 수) :");
		} while (userValue >= minValue || userValue >= maxValue);
		
		
		// 메모리 해제
		sc.close();
	}
}
