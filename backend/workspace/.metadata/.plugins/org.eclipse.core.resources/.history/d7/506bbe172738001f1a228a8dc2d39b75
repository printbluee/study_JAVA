package generic;

class Thing {
	private String id;
	private String name; 
	
	public Thing() {}
	
	public Thing(String id, String name) {
		this.id = id;
		this.name = name;
	}
}

class Tv extends Thing {
	@Override
	public String toString() {
		return "나는 새로운 Tv";
	}
}

class Pen extends Thing {
	@Override
	public String toString() {
		return "나는 평생 사용 가능한 pen이다 !";
	}
}

class Box {
	// 박스 안에는 뭐든지 담을 수 있음
}



public class Ex07 {

	public static void main(String[] args) {

		
		
	}
}
