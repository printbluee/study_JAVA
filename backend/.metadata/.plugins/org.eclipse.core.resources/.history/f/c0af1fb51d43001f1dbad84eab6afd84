package optional;

import java.util.Arrays;
import java.util.List;

public class Ex02 {

	public static String findByName(String name) {
		List <String> result = Arrays.asList("홍길동", "박보검", "이미자");
		
		for (String s : result) {
			if (s.equals(name)) {
				System.out.println(s);
				return s;
			}
		} 
	}
	
	public static void main(String[] args) {

	}
}
